APP_NAME=sensei-ui-test
CONTAINER_NAME=sensei-ui-test

# host port exposed by runtime
PORT=3000

# internal port nginx serves on
CONTAINER_PORT=80

# health endpoint inside container
HEALTH_PATH=/health

# image coordinates expected by the platform
IMAGE=ghcr.io/Deepcoders30/sensei-ui-test
TAG=main

# optional host logs dir if you mount logs
LOGS_DIR=/var/log/sensei-ui-test
